<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: 'Ethan Streadbeck Portfolio', sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Ethan Streadbeck Portfolio", sticky: false) %>
<% else %>
  <%= js add_gritter(flash[:notice], title: "Ethan Streadbeck Portfolio", sticky: false) %>
<% end %>

